{
  "name": "hono-steamer-api",
  "type": "module",
  "version": "0.0.10",
  "license": "MIT",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "node ./dist/src/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "bump": "bumpp",
    "test": "cross-env NODE_ENV=test vitest",
    "build": "tsc && tsc-alias",
    "gen": "drizzle-kit generate",
    "push": "drizzle-kit push",
    "gp": "drizzle-kit generate && drizzle-kit push",
    "stu": "drizzle-kit studio",
    "migrate": "drizzle-kit migrate"
  },
  "dependencies": {
    "@hono/node-server": "^1.13.7",
    "@hono/zod-openapi": "^0.18.3",
    "@scalar/hono-api-reference": "^0.5.165",
    "bumpp": "^9.10.1",
    "dotenv": "^16.4.7",
    "dotenv-expand": "^12.0.1",
    "drizzle-orm": "^0.33.0",
    "drizzle-zod": "^0.5.1",
    "hono": "^4.6.15",
    "hono-pino": "^0.7.0",
    "mysql2": "^3.12.0",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "steamid-resolver": "^1.3.6",
    "stoker": "1.4.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.12.1",
    "@types/node": "^22.10.2",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.24.2",
    "eslint": "^9.17.0",
    "eslint-plugin-format": "^0.1.3",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
